{
  "manifest_version": 2,

  "name": "442 muter",
  "description": "This extension allows you to mute annoying users on the AU fourfourtwo forums",
  "version": "0.5",
  "options_page": "options.html",
  "content_scripts": [
   {
     "matches": ["http://forum.insidesport.com.au/*"],
     "js": ["jquery.js", "mute.js"]
   }
 ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ],
  "web_accessible_resources": [
  "images/music_off.png"
]
}
